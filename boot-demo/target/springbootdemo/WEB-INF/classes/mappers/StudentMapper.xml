<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.lemon.boot.dao.StudentDao">
    <select id="getStudentById" parameterType="int" resultType="cn.lemon.boot.dto.StudentDto">
        select * from lemon_test.student where sno = #{id}
    </select>
</mapper>